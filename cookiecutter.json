{
  "project_name": "Python Boilerplate",
  "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '-') }}",
  "pkg_namespace": "{{ '.'.join(cookiecutter.project_slug.split('.')[1:]) }}",
  "pkg_name": "{{ ''.join(cookiecutter.project_slug.split('.')[:1]) }}",
  "pkg_dir": "{{ '/'.join(cookiecutter.pkg_namespace.split('.'))+'/'+cookiecutter.pkg_name }}",
  "project_short_description": "Skeleton project created by Cookiecutter PyPackage",
  "version": "0.1.0",
  "full_name": "Philippe Giacinti",
  "email": "philippe@giacinti.com",
  "github_username": "giacinti",
  "command_line_interface": ["Click", "No command-line interface"],
  "create_author_file": "y",
  "open_source_license": ["MIT", "BSD-3-Clause", "ISC", "Apache-2.0", "GPL-3.0-only", "Not open source"],
  "install_precommit_hooks": "y",
  "_copy_without_render": [".github/workflows/*.yml"]
}
